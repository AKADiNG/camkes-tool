connector GenericRPC {
    from Procedures;
    to Procedures;
}

procedure Hello {
    char getchar(void);
    int putchar(in char x);
}

component Foo {
    control;
    uses Hello s;
}

component Bar {
    provides Hello t;
}

assembly {
  composition {
    component Foo foo;
    component Bar bar;
    connection GenericRPC con(from foo.s, to bar.t);
  }
}
